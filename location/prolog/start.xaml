        <Button Name="Town" Content="Селение" HorizontalAlignment="Left" Margin="77,287,0,0" VerticalAlignment="Top" Width="75" Visibility="Hidden"/>
        <Button Name="Forest" Content="Лес" HorizontalAlignment="Left" Margin="172,287,0,0" VerticalAlignment="Top" Width="75" Visibility="Hidden"/>
        <Button Name="Conti" Content="Продолжить" HorizontalAlignment="Left" Margin="265,287,0,0" VerticalAlignment="Top" Width="75" Click="Conti_Click"/>
        <TextBox Name="prol_tb" HorizontalAlignment="Left" Height="140" Margin="77,142,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="383" TextChanged="Prol_tb_TextChanged" IsReadOnly="True"/>

    </Grid>
