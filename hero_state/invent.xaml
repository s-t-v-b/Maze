        xmlns:local="clr-namespace:Maze.hero_state"
        mc:Ignorable="d"
        Title="invent" Height="350" Width="450">
    <Grid>
        <Label x:Name="socet1" Content="Label" HorizontalAlignment="Left" Height="27" Margin="26,48,0,0" VerticalAlignment="Top" Width="70"/>
        <Button x:Name="b_socet1" Content="Button" HorizontalAlignment="Left" Margin="101,51,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="socet2" Content="Label" HorizontalAlignment="Left" Margin="26,80,0,0" VerticalAlignment="Top"/>
