        <Button x:Name="b_socet10" Content="Button" HorizontalAlignment="Left" Margin="332,158,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="exit" Content="Button" HorizontalAlignment="Left" Margin="295,205,0,0" VerticalAlignment="Top" Width="75" Click="Exit_Click"/>
    </Grid>
