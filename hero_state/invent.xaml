        <Button x:Name="b_socet7" Content="Button" HorizontalAlignment="Left" Margin="332,53,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="socet8" Content="Label" HorizontalAlignment="Left" Margin="257,82,0,0" VerticalAlignment="Top"/>
        <Button x:Name="b_socet8" Content="Button" HorizontalAlignment="Left" Margin="332,88,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.5,0.5">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.511"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Label x:Name="socet9" Content="Label" HorizontalAlignment="Left" Height="27" Margin="257,120,0,0" VerticalAlignment="Top" Width="70"/>
