        <Label x:Name="socet3" Content="Label" HorizontalAlignment="Left" Height="27" Margin="26,115,0,0" VerticalAlignment="Top" Width="70"/>
        <Button x:Name="b_socet3" Content="Button" HorizontalAlignment="Left" Margin="101,118,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="socet4" Content="Label" HorizontalAlignment="Left" Margin="26,147,0,0" VerticalAlignment="Top"/>
        <Button x:Name="b_soce4" Content="Button" HorizontalAlignment="Left" Margin="101,153,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="socet5" Content="Label" HorizontalAlignment="Left" Height="27" Margin="26,178,0,0" VerticalAlignment="Top" Width="70"/>
